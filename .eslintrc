{
  "env": {
    "node": true,
    "es6": true,
    "commonjs": true,
    "jest": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 9,
    "sourceType": "module",
    "ecmaFeatures": {
      "globalReturn": false,
      "impliedStrict": true,
      "jsx": true
    }
  },
  "extends": "eslint:recommended",
  "rules": {
    "indent": [2, 2, {"SwitchCase": 1}],
    "strict": [2, "never"]
  }
}
